   "source": [
    "import turtle\n",
    "colors=['red','orange','yellow','green','blue','purple']\n",
    "t=turtle.Pen()\n",
    "turtle.bgcolor('black')\n",
    "for x in range (360) :\n",
    "    t.pencolor(colors[x%6])\n",
    "    t.width(x/100+1)\n",
    "    t.forward(x)\n",
    "    t.left(59)"
   ]
